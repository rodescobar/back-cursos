{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "node_modules/swagger-ui-dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}